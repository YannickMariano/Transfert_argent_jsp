    <servlet-class>net.javaguides.transfert.web.ClientServlet</servlet-class>
  </servlet>
  <servlet>
    <description></description>
    <display-name>TauxServlet</display-name>
    <servlet-name>TauxServlet</servlet-name>
    <servlet-class>net.javaguides.transfert.web.TauxServlet</servlet-class>
  </servlet>
  <servlet-mapping>
    <servlet-name>TauxServlet</servlet-name>
    <url-pattern>/new_taux</url-pattern>
