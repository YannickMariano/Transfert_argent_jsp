    <url-pattern>/delete_frais</url-pattern>
    <url-pattern>/edit_frais</url-pattern>
    <url-pattern>/update_frais</url-pattern>
  </servlet-mapping>
  <servlet>
    <description></description>
    <display-name>transfertServlet</display-name>
